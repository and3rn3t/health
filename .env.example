# Auth0 Configuration for VitalSense Health App
# Copy this file to .env.local and fill in your Auth0 application details

# Auth0 Domain (e.g., your-tenant.auth0.com)
VITE_AUTH0_DOMAIN=your-domain.auth0.com

# Auth0 Client ID (from your Auth0 Application)
VITE_AUTH0_CLIENT_ID=your-client-id

# Auth0 Audience (API Identifier for your Auth0 API)
VITE_AUTH0_AUDIENCE=https://vitalsense-health-api

# Application URLs
VITE_AUTH0_REDIRECT_URI=http://127.0.0.1:8789/callback
VITE_AUTH0_LOGOUT_URI=http://127.0.0.1:8789/login

# Environment
VITE_ENVIRONMENT=development

# Example production configuration:
# VITE_AUTH0_DOMAIN=vitalsense-prod.auth0.com
# VITE_AUTH0_CLIENT_ID=prod-client-id
# VITE_AUTH0_AUDIENCE=https://api.vitalsense.app
# VITE_AUTH0_REDIRECT_URI=https://app.vitalsense.com/callback
# VITE_AUTH0_LOGOUT_URI=https://app.vitalsense.com/login
# VITE_ENVIRONMENT=production
