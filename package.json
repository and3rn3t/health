{
  "name": "vitalsense-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "kill": "fuser -k 5000/tcp",
    "build": "vite build && vite build --config vite.worker.config.ts",
    "build:app": "tsc --noEmit && vite build",
    "build:worker": "vite build --config vite.worker.config.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "lint:all": "pwsh -NoProfile -File scripts/lint-all.ps1",
    "lint:all:fix": "pwsh -NoProfile -File scripts/lint-all.ps1 -Fix",
    "lint:swift": "pwsh -NoProfile -File ios/scripts/swift-lint-windows.ps1",
    "optimize": "vite optimize",
    "preview": "vite preview",
    "wrangler": "wrangler",
    "deploy:dev": "npm run build && wrangler deploy --env development",
    "deploy:prod": "npm run build && wrangler deploy --env production",
    "dns:setup": "pwsh -NoProfile -File scripts/dns-setup.ps1",
    "dns:phase1": "pwsh -NoProfile -File scripts/dns-setup.ps1 -Phase 1",
    "dns:phase2": "pwsh -NoProfile -File scripts/dns-setup.ps1 -Phase 2",
    "dns:phase3": "pwsh -NoProfile -File scripts/dns-setup.ps1 -Phase 3",
    "dns:cleanup": "pwsh -NoProfile -File scripts/dns-setup.ps1 -CleanUp",
    "platform:deploy": "pwsh -NoProfile -File scripts/deploy-platform.ps1",
    "platform:phase1": "pwsh -NoProfile -File scripts/deploy-platform.ps1 -Phase 1",
    "platform:phase2": "pwsh -NoProfile -File scripts/deploy-platform.ps1 -Phase 2",
    "platform:phase3": "pwsh -NoProfile -File scripts/deploy-platform.ps1 -Phase 3",
    "platform:verify": "pwsh -NoProfile -File scripts/deploy-platform.ps1 -Verify",
    "cf:dev": "wrangler dev --env development",
    "cf:tail": "wrangler tail",
    "cf:kv": "wrangler kv",
    "start:worker": "wrangler dev --env development --local",
    "probe:dev": "pwsh -NoProfile -File scripts/probe.ps1 -HostUrl http://127.0.0.1 -Port 8787 -UserId demo-user",
    "probe:dev:nodejs": "node scripts/node/health/probe.js --port 8787 --user-id demo-user --verbose",
    "probe:simple": "node scripts/node/health/simple-probe.js --port 8787",
    "probe:simple:8788": "node scripts/node/health/simple-probe.js --port 8788",
    "task:run": "node scripts/node/dev/task-runner.js",
    "start:dev": "node scripts/node/dev/start-dev.js",
    "lint:ts": "node scripts/node/dev/lint-runner.js --typescript",
    "lint:swift:node": "node scripts/node/dev/lint-runner.js --swift",
    "lint:all:node": "node scripts/node/dev/lint-runner.js --all",
    "config:validate": "node scripts/node/utils/config-validator.js",
    "config:fix": "node scripts/node/utils/config-validator.js --fix",
    "test:dev": "node scripts/node/test/test-runner.js --dev",
    "test:quick": "node scripts/node/test/test-runner.js --quick",
    "test:full": "node scripts/node/test/test-runner.js --full",
    "test:api": "node scripts/node/test/test-runner.js --api",
    "test:prod": "node scripts/node/test/test-runner.js --prod",
    "deploy:platform:node": "node scripts/node/deploy/platform-deploy.js",
    "deploy:platform:phase1:node": "node scripts/node/deploy/platform-deploy.js --phase 1",
    "deploy:platform:phase2:node": "node scripts/node/deploy/platform-deploy.js --phase 2",
    "deploy:platform:phase3:node": "node scripts/node/deploy/platform-deploy.js --phase 3",
    "deploy:platform:verify:node": "node scripts/node/deploy/platform-deploy.js --verify",
    "deploy:platform:dry": "node scripts/node/deploy/platform-deploy.js --dry-run --verbose",
    "dns:setup:node": "node scripts/node/deploy/dns-setup.js",
    "dns:setup:phase1:node": "node scripts/node/deploy/dns-setup.js --phase 1",
    "dns:setup:phase2:node": "node scripts/node/deploy/dns-setup.js --phase 2",
    "dns:setup:phase3:node": "node scripts/node/deploy/dns-setup.js --phase 3",
    "dns:cleanup:node": "node scripts/node/deploy/dns-setup.js --cleanup",
    "dns:dry": "node scripts/node/deploy/dns-setup.js --dry-run --verbose",
    "deploy:vitalsense": "node scripts/node/deploy/vitalsense-deploy.js",
    "deploy:vitalsense:full": "node scripts/node/deploy/vitalsense-deploy.js --deploy",
    "deploy:vitalsense:dev": "node scripts/node/deploy/vitalsense-deploy.js --dev-server",
    "pretest": "vite build --config vite.worker.config.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "hooks:setup": "pwsh -NoProfile -File scripts/setup-git-hooks.ps1",
    "ios:setup": "pwsh -NoProfile -File ios/scripts/setup-ios-dev-windows.ps1 -All",
    "ios:lint": "pwsh -NoProfile -File ios/scripts/swift-lint-windows.ps1",
    "ios:format": "pwsh -NoProfile -File ios/scripts/swift-format-windows.ps1",
    "ios:check": "pwsh -NoProfile -File ios/HealthKitBridge/scripts/Check-SwiftErrors.ps1",
    "ios:build-sim": "pwsh -NoProfile -File ios/scripts/ios-build-simulator.ps1",
    "ios:deps": "pwsh -NoProfile -File ios/scripts/swift-dependency-analyzer.ps1",
    "ios:perf": "pwsh -NoProfile -File ios/scripts/swift-performance-analyzer.ps1",
    "ios:deps-graph": "pwsh -NoProfile -File ios/scripts/swift-dependency-analyzer.ps1 -ShowGraph",
    "production:setup": "pwsh -NoProfile -File scripts/setup-production-infrastructure.ps1 -All",
    "production:deploy": "pwsh -NoProfile -File scripts/setup-production-infrastructure.ps1 -Deploy",
    "production:secrets": "pwsh -NoProfile -File scripts/setup-production-infrastructure.ps1 -SetupSecrets",
    "production:observability": "pwsh -NoProfile -File scripts/setup-production-infrastructure.ps1 -EnableObservability",
    "production:dns": "pwsh -NoProfile -File scripts/setup-production-infrastructure.ps1 -ConfigureDNS",
    "production:waf": "pwsh -NoProfile -File scripts/setup-production-infrastructure.ps1 -SetupWAF",
    "production:verify": "pwsh -NoProfile -File scripts/setup-production-infrastructure.ps1 -Verify",
    "ios:perf-detail": "pwsh -NoProfile -File ios/scripts/swift-performance-analyzer.ps1 -Detailed",
    "ios:quick": "pwsh -NoProfile -File ios/scripts/ios-workflow.ps1 -QuickCheck",
    "ios:full": "pwsh -NoProfile -File ios/scripts/ios-workflow.ps1 -FullCheck",
    "ios:fix": "pwsh -NoProfile -File ios/scripts/ios-workflow.ps1 -FixIssues",
    "ios:ready": "pwsh -NoProfile -File ios/scripts/ios-workflow.ps1 -PrepareForBuild",
    "ios:test": "pwsh -NoProfile -File ios/scripts/ios-test-runner.ps1 -All",
    "ios:test:unit": "pwsh -NoProfile -File ios/scripts/ios-test-runner.ps1 -Unit",
    "ios:test:ui": "pwsh -NoProfile -File ios/scripts/ios-test-runner.ps1 -UI",
    "ios:test:performance": "pwsh -NoProfile -File ios/scripts/ios-test-runner.ps1 -Performance",
    "ios:test:coverage": "pwsh -NoProfile -File ios/scripts/ios-test-runner.ps1 -All -Coverage",
    "ios:test:device": "pwsh -NoProfile -File ios/scripts/ios-test-runner.ps1 -All -Device",
    "ios:test:verbose": "pwsh -NoProfile -File ios/scripts/ios-test-runner.ps1 -All -Verbose",
    "ios:test:env-setup": "pwsh -NoProfile -File ios/scripts/test-environment-setup.ps1 -All",
    "ios:test:env-check": "pwsh -NoProfile -File ios/scripts/test-environment-setup.ps1 -ValidateEnvironment",
    "ios:test:clean": "pwsh -NoProfile -File ios/scripts/test-environment-setup.ps1 -CleanTestData",
    "ios:test:specific": "pwsh -NoProfile -File ios/scripts/ios-test-runner.ps1 -Unit -TestCase",
    "app:deploy": "pwsh -NoProfile -File ios/scripts/main-app-deploy.ps1 -Deploy",
    "app:deploy:dev": "pwsh -NoProfile -File ios/scripts/main-app-deploy.ps1 -Environment development -Deploy",
    "app:deploy:staging": "pwsh -NoProfile -File ios/scripts/main-app-deploy.ps1 -Environment staging -Deploy",
    "app:deploy:prod": "pwsh -NoProfile -File ios/scripts/main-app-deploy.ps1 -Environment production -Deploy",
    "app:status": "pwsh -NoProfile -File ios/scripts/main-app-deploy.ps1 -Status",
    "app:sync": "pwsh -NoProfile -File ios/scripts/main-app-deploy.ps1 -Sync",
    "app:quick": "pwsh -NoProfile -File ios/scripts/main-app-deploy.ps1 -Quick"
  },
